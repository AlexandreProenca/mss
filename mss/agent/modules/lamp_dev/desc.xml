<module id="lamp_dev">
    <name>LAMP developpement packages (PHP, Perl/CGI)</name>
    <desc>A set of open source tools to run dynamic web sites (scripting, php, modules, ...)</desc>
    <packages>
        <target name="all">
            <rpm>apache-mod_perl</rpm>
            <rpm>apache-mod_php</rpm>
            <rpm>php-dom</rpm>
            <rpm>php-xml</rpm>
            <rpm>php-xmlrpc</rpm>
            <rpm>php-cli</rpm>
            <rpm>php-mysql</rpm>
            <rpm>php-pgsql</rpm>
            <rpm>php-sqlite</rpm>
        </target>
    </packages>
</module>
