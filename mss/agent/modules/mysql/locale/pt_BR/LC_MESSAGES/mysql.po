#: modules/mysql/desc.xml2(name)
msgid "MySQL database"
msgstr "Banco de dados MySQL"

#: modules/mysql/desc.xml3(desc)
msgid "MySQL relational database"
msgstr "Banco de dados relacional MySQL"

#: modules/mysql/desc.xml6(rpm)
msgid "mysql"
msgstr "mysql"

#: modules/mysql/desc.xml7(rpm)
msgid "mysql-client"
msgstr "mysql-client"

#: modules/mysql/desc.xml11(module)
msgid "mysqlmax"
msgstr "mysqlmax"

#: modules/mysql/desc.xml15(label)
msgid "Current password"
msgstr "Senha atual"

#: modules/mysql/desc.xml16(help)
msgid "Current MySQL root password. Leave blank for first installation."
msgstr "Senha de root atual do MySQL. Deixe em branco para a primeira instalação."

#: modules/mysql/desc.xml19(label)
msgid "New password"
msgstr "Nova senha"

#: modules/mysql/desc.xml20(help)
msgid "New MySQL root password. Set here the new password."
msgstr "Nova senha de root do MySQL. Defina aqui a nova senha."

#: modules/mysql/desc.xml23(text)
msgid "OK, successfully used password, moving on..."
msgstr "OK, senha utilizada com sucesso, direcionando..."

#: modules/mysql/desc.xml24(text)
msgid "Invalid current MySQL password."
msgstr "Senha atual do MySQL incorreta."

#: modules/mysql/desc.xml25(text)
msgid "Setting the root password ensures that nobody can log into the MySQL"
msgstr "Definir a senha de root garante que ninguém pode entrar no MySQL"

#: modules/mysql/desc.xml26(text)
msgid "root user without the proper authorisation."
msgstr "usuário root sem a devida autorização."

#: modules/mysql/desc.xml27(text)
msgid "Password updated successfully!"
msgstr "Senha atualizada com sucesso!"

#: modules/mysql/desc.xml28(text)
msgid "Reloading privilege tables.."
msgstr "Recarregar as tabelas de privilégio.."

#: modules/mysql/desc.xml29(text)
msgid "Password update failed!"
msgstr "A atualização de senha falhou!"

#: modules/mysql/desc.xml30(text)
msgid "Removing anonymous users"
msgstr "Removendo usuários anônimos"

#: modules/mysql/desc.xml31(text)
msgid "... Success!"
msgstr "... Sucesso!"

#: modules/mysql/desc.xml32(text)
msgid "... Failed!"
msgstr "... Falhou!"

#: modules/mysql/desc.xml33(text)
msgid "Disallowing root login remotely"
msgstr "Não permitir login como root remotamente"

#: modules/mysql/desc.xml34(text)
msgid "Remove test database and access to it"
msgstr "Remova o banco de dados de teste e acesse-o"

#: modules/mysql/desc.xml35(text)
msgid "- Dropping test database..."
msgstr "- Destruindo banco de dados de teste..."

#: modules/mysql/desc.xml36(text)
msgid "... Failed! Not critical, keep moving..."
msgstr "... Falhou! Não crítico, manter ação..."

#: modules/mysql/desc.xml37(text)
msgid "- Removing privileges on test database..."
msgstr "- Remoção de privilégios no banco de dados de teste..."

#: modules/mysql/desc.xml38(text)
msgid "Reloading the privilege tables"
msgstr "Recargando as tabelas de privilégio"

#: modules/mysql/desc.xml39(text)
msgid "Aborting!"
msgstr "Abortar!"

#: modules/mysql/desc.xml40(text)
msgid "Cleaning up..."
msgstr "Limpeza..."

#: modules/mysql/desc.xml41(text)
msgid "MySQL is running"
msgstr "MySQL está executando"

#: modules/mysql/desc.xml42(text)
msgid "- A password has been set for the root user"
msgstr "- Uma senha foi definida para o usuário root"

#: modules/mysql/desc.xml43(text)
msgid "- Anonymous users were removed"
msgstr "- Usuários anônimos foram removidos"

#: modules/mysql/desc.xml44(text)
msgid "- Test database has been removed"
msgstr "- Banco de dados de teste foi removido"

#: modules/mysql/desc.xml45(text)
msgid "- Remote login for root has been disabled"
msgstr "- Login remoto para o root foi desabilitado"

