<module id="openldap_kerberos">
  <name>LDAP + Kerberos authentication</name>
  <desc>LDAP + Kerberos authentication server</desc>
  <conflicts>
    <module>mds_mmc</module>
  </conflicts>  
  <deps>
    <module>opendlap</module>
    <module>kerberos</module>
  </deps>
  <packages>
    <target name="i686">
      <rpm>libsasl2-plug-gssapi</rpm>
    </target>
    <target name="x86_64">
      <rpm>lib64sasl2-plug-gssapi</rpm>
    </target>
  </packages>
</module>
