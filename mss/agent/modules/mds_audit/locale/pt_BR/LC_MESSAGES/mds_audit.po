#: modules/mds_audit/desc.xml2(name)
msgid "Audit module for MDS"
msgstr "Módulo de auditoria para o MDS"

#: modules/mds_audit/desc.xml3(desc)
msgid "Track any changes made on the MDS interface"
msgstr "Acompanhar todas as mudanças feitas na interface do MDS"

#: modules/mds_audit/desc.xml5(module)
msgid "mds_mmc"
msgstr "mds_mmc"

#: modules/mds_audit/desc.xml6(module)
msgid "mysql"
msgstr "mysql"

#: modules/mds_audit/desc.xml10(label)
msgid "MySQL root password"
msgstr "Senha de root do MySQL"

#: modules/mds_audit/desc.xml11(help)
msgid "Used to create the audit database"
msgstr "Usado para criar o banco de dados de auditoria"

#: modules/mds_audit/desc.xml14(label)
