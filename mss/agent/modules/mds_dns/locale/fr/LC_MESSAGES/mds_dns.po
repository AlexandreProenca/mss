msgstr ""

#: modules/mds_dns/desc.xml31(text)
msgid "Service named reloaded succesfully."
msgstr "Le service named a redémarré."

#: modules/mds_dns/desc.xml32(text)
msgid "Service named fails restarting. Check /var/log/syslog"
msgstr "Le service named n'a pas redémarré. Vérifiez /var/log/syslog"

#: modules/mds_dns/desc.xml33(text)
msgid "The DNS service is running."
msgstr ""

#: modules/mds_dns/desc.xml34(text)
