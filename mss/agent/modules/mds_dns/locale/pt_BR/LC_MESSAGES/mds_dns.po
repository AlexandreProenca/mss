#: modules/mds_dns/desc.xml3(name)
msgid "DNS service"
msgstr ""

#: modules/mds_dns/desc.xml4(desc)
msgid "Resolve computer names on your network"
msgstr "Resolver nomes de computador em sua rede"

#: modules/mds_dns/desc.xml10(rpm)
msgid "bind"
msgstr "bind"

#: modules/mds_dns/desc.xml11(rpm)
msgid "bind-utils"
msgstr "bind-utils"

