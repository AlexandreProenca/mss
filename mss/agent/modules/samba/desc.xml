<module id="samba">
  <name>SAMBA file server</name>
  <desc>Share files and printers on a Microsoft network</desc>
  <conflicts>
    <module>mds_smb</module>
  </conflicts>
  <packages>
    <target name="all">
      <rpm>samba-server</rpm>
      <rpm>samba-client</rpm>
      <rpm>samba-common</rpm>
      <rpm>samba-doc</rpm>
      <rpm>smbldap-tools</rpm>
    </target>
  </packages>
</module>
