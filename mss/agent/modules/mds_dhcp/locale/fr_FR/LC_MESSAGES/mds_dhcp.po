#: modules/mds_dhcp/desc.xml2(name)
msgid "DHCP service"
msgstr "Service DHCP"

#: modules/mds_dhcp/desc.xml3(desc)
msgid "Configure automatically your client computers IP settings"
msgstr "Configuration réseau automatique de vos clients"

#: modules/mds_dhcp/desc.xml9(rpm)
msgid "dhcp-server"
msgstr "dhcp-server"

#: modules/mds_dhcp/desc.xml10(rpm)
msgid "mmc-web-network"
msgstr "mmc-web-network"

#: modules/mds_dhcp/desc.xml11(rpm)
msgid "python-mmc-network"
msgstr "python-mmc-network"

#: modules/mds_dhcp/desc.xml15(module)
msgid "mds_mmc"
msgstr "mds_mmc"

#: modules/mds_dhcp/desc.xml19(label)
msgid "Allow DHCP requests from internal networks"
msgstr "Autoriser les requêtes DHCP depuis les réseaux internes"

#: modules/mds_dhcp/desc.xml20(help)
msgid ""
"Configure the firewall to accept DHCP requests on interfaces configured as "
"'internal'"
msgstr ""
"Configure le firewall pour accepter les requêtes DHCP sur les interfaces "
"internes"

#: modules/mds_dhcp/desc.xml24(label)
msgid "Allow DHCP requests from external networks"
msgstr "Autoriser les requêtes DHCP depuis les réseaux externes"

#: modules/mds_dhcp/desc.xml25(help)
msgid ""
"Configure the firewall to accept DHCP requests on interfaces configured as "
"'external'"
msgstr ""
"Configure le firewall pour accepter les requêtes DHCP sur les interfaces "
"externes"

#: modules/mds_dhcp/desc.xml29(text)
msgid "Configuration done. (/etc/mmc/plugins/network.ini updated)"
msgstr "Configuration faite. (/etc/mmc/plugins/network.ini mis à jour)"

#: modules/mds_dhcp/desc.xml30(text)
msgid ""
"Error while configuring the dhcp service. (/etc/mmc/plugins/network.ini)"
msgstr "Erreur lors de la configuration. (/etc/mmc/plugins/network.ini)"

#: modules/mds_dhcp/desc.xml31(text)
msgid "Configuration of dhcp-server done. (/etc/dhcpd.conf updated)"
msgstr "Configuration de dhcp-server faite. (/etc/dhcpd.conf mis à jour)"

#: modules/mds_dhcp/desc.xml32(text)
msgid "Error while configuring dhcp-server. (/etc/dhcpd.conf)"
msgstr "Error lors de la configuration de dhcp-server. (/etc/dhcpd.conf)"

#: modules/mds_dhcp/desc.xml33(text)
msgid "Service mmc-agent reloaded succesfully."
msgstr ""

#: modules/mds_dhcp/desc.xml34(text)
msgid "Service mmc-agent fails restarting. Check /var/log/mmc/mmc-agent.log"
msgstr ""

#: modules/mds_dhcp/desc.xml35(text)
msgid "The DHCP service is installed."
msgstr "Le service DHCP est installé."

#: modules/mds_dhcp/desc.xml36(text)
msgid ""
"You can now configure your DHCP settings from the management interface : "
"https://@HOSTNAME@/mmc/"
msgstr ""
"Vous pouvez maintenant configurer vos paramètres DHCP depuis l'interface de "
"gestion : "

