#: modules/mds_dhcp/desc.xml2(name)
msgid "DHCP server with MDS backend"
msgstr "Servidor DHCP com backend MDS"

#: modules/mds_dhcp/desc.xml3(desc)
msgid "Configure your client computers IP settings"
msgstr "Configure as suas preferências de IP dos computadores de clientes"

#: modules/mds_dhcp/desc.xml6(rpm)
msgid "dhcp-server"
msgstr "dhcp-server"

#: modules/mds_dhcp/desc.xml7(rpm)
msgid "mmc-web-network"
msgstr "mmc-web-network"

#: modules/mds_dhcp/desc.xml8(rpm)
msgid "python-mmc-network"
msgstr "python-mmc-network"

#: modules/mds_dhcp/desc.xml12(module)
msgid "mds_mmc"
msgstr "mds_mmc"

#: modules/mds_dhcp/desc.xml14(text)
msgid "MMC interface is not installed."
msgstr "A interface MMC não está instalada."

#: modules/mds_dhcp/desc.xml15(text)
msgid "Can't continue."
msgstr "Não pode continuar."

#: modules/mds_dhcp/desc.xml16(text)
msgid "MDS configuration done. (/etc/mmc/plugins/network.ini updated)"
msgstr "Configuração do MDS feita. (/etc/mmc/plugins/network.ini atualizado)"

#: modules/mds_dhcp/desc.xml17(text)
msgid "Error while configuring MDS. (/etc/mmc/plugins/network.ini)"
msgstr "Erro ao configurar o MDS. (/etc/mmc/plugins/network.ini)"

#: modules/mds_dhcp/desc.xml18(text)
msgid "Configuration of dhcp-server done. (/etc/dhcpd.conf updated)"
msgstr "Configuração do dhcp-server feito. (/etc/dhcpd.conf atualizado)"

#: modules/mds_dhcp/desc.xml19(text)
msgid "Error while configuring dhcp-server. (/etc/dhcpd.conf)"
msgstr "Erro ao configurar o dhcp-server. (/etc/dhcpd.conf)"

#: modules/mds_dhcp/desc.xml20(text)
msgid "Service MMC reloaded succesfully."
msgstr "O serviço MMC foi recarregado com sucesso."

#: modules/mds_dhcp/desc.xml21(text)
msgid "Service MMC fails restarting. Check /var/log/mmc/mmc-agent.log"
msgstr "O serviço MMC falhou ao reiniciar. Verifique /var/log/MMC/MMC-agent.log"

#: modules/mds_dhcp/desc.xml22(text)
msgid "DHCP module is activated in the MDS interface."
msgstr "O módulo DHCP está ativado na interface do MDS."

#: modules/mds_dhcp/desc.xml23(text)
msgid "You can now configure your DHCP settings from the MDS interface : http://@HOSTNAME@/mmc/"
msgstr "Agora você pode configurar suas definições de DHCP a partir da interface MDS em http://@HOSTNAME@/mmc/"

