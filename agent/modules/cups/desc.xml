<module id="cups">
  <name>Printing server</name>
  <desc>Install, configure and share network printers with cups</desc>
  <packages>
    <target name="all">
      <rpm>cups</rpm>
      <rpm>cups-drivers</rpm>
      <rpm>cups-windows</rpm>
      <rpm>foomatic-filters</rpm>
      <rpm>hplip-hpijs-ppds</rpm>
      <rpm>postscript-ppds</rpm>
      <rpm>hplip</rpm>
    </target>
  </packages>
</module>
